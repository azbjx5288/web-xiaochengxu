<view class="flex-wrp" style="display: flex;align-items: center;flex-direction:row;height:30px;margin-left:15px;padding:5px;">
  <image src="../../images/picture.png" style="height: 20px;width:20px;"></image>
  <text type="default"  style="font-size: 16px;margin-left:10px;" >{{picture}}</text>
</view>

<view class = "company_image">

    <image src="../../images/01.jpg"class="userinfo-avatar" bindtap="bindComImg1"  background-size="cover"mode="aspectFill"></image>
    <image src="../../images/02.jpg" class="userinfo-avatar" bindtap="bindComImg2"  background-size="cover"mode="aspectFill"></image>
    <image src="../../images/03.jpg" class="userinfo-avatar"  bindtap="bindComImg3"  background-size="cover"mode="aspectFill"></image>
    <image src="../../images/04.jpg" class="userinfo-avatar" bindtap="bindComImg4"  background-size="cover"mode="aspectFill"></image>
    <image src="../../images/05.jpg" class="userinfo-avatar" bindtap="bindComImg5"  background-size="cover"mode="aspectFill"></image>
    <image src="../../images/06.jpg" class="userinfo-avatar" bindtap="bindComImg6"  background-size="cover"mode="aspectFill"></image>
    <image src="../../images/07.jpg" class="userinfo-avatar" bindtap="bindComImg7"  background-size="cover"mode="aspectFill"></image>
    <image src="../../images/08.jpg" class="userinfo-avatar" bindtap="bindComImg8"  background-size="cover"mode="aspectFill"></image>
    <image src="../../images/09.jpg" class="userinfo-avatar" bindtap="bindComImg9"  background-size="cover"mode="aspectFill"></image>
</view>


