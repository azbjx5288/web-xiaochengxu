.container{
  background-color: #f8f8f8;
}

.binner{
  width: 100%;
  height: 150px;
  box-sizing: border-box;
}

.list{
  border-bottom: 1px solid #dadada;
  display: block;
  padding-top:20px;
  padding-bottom: 20px;
  padding-left: 12px;
  padding-right: 12px;
  margin-top: 1px;
  background-color: #fff;
}

.list .title{
 display: block;
 font-size: 14px;
 font-weight: 700;
 height: 40px;
  line-height: 40px;
}
.list .desc{
  color: #999;
  font-size: 14px;
  display: block;
  height: 30px;
  line-height: 30px;
}